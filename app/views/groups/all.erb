<% if @user.groups.empty? %>
  <h1>You have not made any groups yet</h1>
<% else %>

<h1>Here are your groups:</h1>
  <% @user.groups.each do |group| %>
    <% cohort = Cohort.find(group.cohort_id).name %>
    <br><a href="/groups/<%= cohort %>/<%= group.cohort_id %>">Groups for <%= cohort %></a>
  <% end %>
<% end %>